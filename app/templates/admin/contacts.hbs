<h1>Contacts</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th>Message</th>
      <th class="col-md-1">Delete</th>
    </tr>
  </thead>
  <tbody>
  {{#each model as |contact|}}
    <tr>
      <th>{{contact.email}}</th>
      <td>{{contact.message}}</td>
      <td><button class="btn btn-danger btn-s" {{action 'deleteContact' contact}}>Delete</button></td>
    </tr>
  {{/each}}
  </tbody>
</table>